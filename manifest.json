{
    "name": "WorkTimeCaculator",
    "description": "Average work time calculator",
    "version": "0.3.7",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["dataLoad.js"]
        }
    ],
    "action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "background",
        "storage"
    ],
    "host_permissions": [
        "*://gwa.oneunivrs.com//*"
    ]
}