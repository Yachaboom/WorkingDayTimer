{
    "name": "WorkTimeCaculator",
    "description": "Average work time calculator",
    "version": "0.2",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["getInfo.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },

    "action":
    {
        "default_icon": "icon.png"
    },

    "permissions": ["tabs", "activeTab", "scripting", "background"],
   
    "host_permissions": [
      "*://gwa.oneunivrs.com//*"
    ]
}